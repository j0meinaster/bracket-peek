{
	"name": "bracket-peek",
	"displayName": "Bracket Peek",
	"description": "Preview the opening line of code matching the inspected closing bracket",
	"version": "1.2.0",
	"publisher": "jomeinaster-chrisant",
	"icon": "logo.jpg",
	"keywords": [ "bracket", "closing", "opening", "peek", "preview", "highlight", "curly", "match", "tag", "markup",
		"sass", "scss", "css", "styles", "typescript", "javascript", "java", "php", "c", "dart", "cs", "csharp", "cpp", 
		"c plus plus", "c sharp", "html", "xml" ],
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:scss",
		"onLanguage:css",
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:json",
		"onLanguage:dart",
		"onLanguage:java",
		"onLanguage:php",
		"onLanguage:c",
		"onLanguage:cpp",
		"onLanguage:csharp",
		"onLanguage:objective-c",
		"onLanguage:objective-cpp",
		"onLanguage:html",
		"onLanguage:xml"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "bracket-peek",
				"properties": {
					"bracket-peek.peekLocation": {
						"type": "string",
						"default": "top line",
						"enum": [
							"top line",
							"closing line"
						],
						"description": "where to show the bracket peek"
					},
					"bracket-peek.debugMode": {
						"type": "boolean",
						"default": false,
						"description": "enable debug logging"
					}
				}
			}
		]
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/j0meinaster/bracket-peek.git"
	}
}
